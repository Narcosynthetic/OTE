/****** Object:  StoredProcedure [dbo].[GetSapsDetails]    Script Date: 13-Nov-16 9:03:56 PM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
create PROCEDURE [dbo].[GetSapsDetails] 
	
AS
BEGIN
	SET NOCOUNT ON;
			
	SELECT s.[SAP], t.[TyposDescr]
     
	FROM [dbo].[Sap] s
	INNER JOIN [dbo].[TypoiMhxanhmatwn] t
	ON s.TyposMhxanhmatosId = t.id
	
     
END
